		<command:example>
			<maml:title>--------------------------  Adding the details  --------------------------</maml:title>
			<maml:introduction>
				<maml:paragraph>PS C:\&gt;</maml:paragraph>
			</maml:introduction>
			<dev:code>$ProgressBar = New-ProgressBar -MaterialDesign

1..100 | foreach {

    Write-ProgressBar -ProgressBar $ProgressBar `
                        -Activity &quot;Counting to 100&quot; `
                        -PercentComplete $_ `
                        -Status &quot;Currently couting $_ out of 100&quot; `
                        -CurrentOperation &quot;$_&quot; `
                        -SecondsRemaining ( ( 200 * ( 100 - $_ ) ) / 1000 )

    Start-Sleep -Milliseconds 200

}

Close-ProgressBar $ProgressBar</dev:code>
